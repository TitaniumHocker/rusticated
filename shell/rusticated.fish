# rusticated fish shell theme by nightsense (https://github.com/nightsense/rusticated)

# normal text
set fish_color_host              433c33
set fish_color_normal            433c33
set fish_pager_color_completion  433c33
set fish_pager_color_description 433c33

# muted text
set fish_color_autosuggestion    62584c
set fish_color_comment           62584c

# reverse muted
set fish_pager_color_progress    d7cfc7 --background=#62584c

# underlined text
set fish_color_valid_path        --underline

# highlit text
set fish_color_match             --background=#9e9486
set fish_color_search_match      --background=#9e9486
set fish_color_selection         --background=#9e9486

# red errors/warnings
set fish_color_cancel            9f3249
set fish_color_cwd_root          9f3249
set fish_color_error             9f3249

# gold special elements
set fish_color_redirection       795200
set fish_color_escape            795200
set fish_color_operator          795200
set fish_color_end               795200

# green commands/status
set fish_color_command           22671b
set fish_color_cwd               22671b
set fish_color_user              22671b

# cyan parameters
set fish_color_param             006466

# blue quoted strings
set fish_color_quote             005b9e

# magenta pager name match
set fish_pager_color_prefix      794392

