# rusticated fish shell theme by nightsense (https://github.com/nightsense/rusticated)

# normal text
set fish_color_host              463f32
set fish_color_normal            463f32
set fish_pager_color_completion  463f32
set fish_pager_color_description 463f32

# muted text
set fish_color_autosuggestion    60594b
set fish_color_comment           60594b

# reverse muted
set fish_pager_color_progress    d3d0cc --background=#60594b

# underlined text
set fish_color_valid_path        --underline

# highlit text
set fish_color_match             --background=#9a9283
set fish_color_search_match      --background=#9a9283
set fish_color_selection         --background=#9a9283

# red errors/warnings
set fish_color_cancel            874749
set fish_color_cwd_root          874749
set fish_color_error             874749

# gold special elements
set fish_color_redirection       6b5729
set fish_color_escape            6b5729
set fish_color_operator          6b5729
set fish_color_end               6b5729

# green commands/status
set fish_color_command           35633a
set fish_color_cwd               35633a
set fish_color_user              35633a

# cyan parameters
set fish_color_param             00646a

# blue quoted strings
set fish_color_quote             325c89

# magenta pager name match
set fish_pager_color_prefix      744b78

